{# Template used by ez_legacy_render_js #}
{% block ez_legacy_render_js %}
    {% for item in js_files %}
        <script type="text/javascript" src="{{ asset( item ) }}"></script>
    {%endfor %}

    {% for item in js_code_lines %}
        <script type="text/javascript">
            {{item|raw}}
        </script>
    {%endfor %}
{% endblock %}
