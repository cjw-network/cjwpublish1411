    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "http://www.hautelooktech.com/",
    "authors": [
        {
            "name": "Baldur Rensch",
            "email": "brensch@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/framework-bundle": "~2.1",
        "hautelook/templated-uri-router": "~2.0"
    },
    "autoload": {
        "psr-0": { "Hautelook\\TemplatedUriBundle": "" }
    },
    "target-dir": "Hautelook/TemplatedUriBundle",
    "extra": {
